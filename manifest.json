{
  "name": "Tab Mux",
  "version": "0.1.0",
  "permissions": [
    "storage"
  ],
  "description": "A better way to manage tabs",
  "background": {
    "scripts": [
      "background_scripts/main.js"
    ],
    "persistent": false
  },
  "page_action": {
    "default_popup": "pages/popup.html"
  },
  "commands": {
    "save-current-context": {
      "suggested_key": {
        "default": "Ctrl+Shift+S",
        "mac": "Command+Shift+S"
      },
      "description": "Save tabs and open a new window"
    }
  },
  "manifest_version": 2
}